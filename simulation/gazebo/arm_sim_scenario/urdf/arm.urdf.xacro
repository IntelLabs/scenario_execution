            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://moveit_resources_panda_description/meshes/collision/link0.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="3.06357" />
            <inertia ixx="0.3" ixy="0.0" ixz="0.0" iyy="0.3" iyz="0.0" izz="0.3" />
        </inertial>
