    <link name="panda_leftfinger">
        <visual>
            <geometry>
                <mesh filename="package://moveit_resources_panda_description/meshes/visual/finger.dae" />
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://moveit_resources_panda_description/meshes/collision/finger.stl" />
            </geometry>
        </collision>
