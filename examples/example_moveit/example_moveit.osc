import osc.ros

scenario example_moveit:
    do parallel:
        serial:
            move_to_joint_pose() with:
                keep(it.joint_pose == '[-1.5708, 0.0, 0.0, 0.0, 0.0]')
            wait elapsed(3s)
            emit end
        time_out: serial:
            wait elapsed(60s)
            emit fail