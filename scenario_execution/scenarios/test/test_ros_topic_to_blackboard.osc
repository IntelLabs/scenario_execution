import osc.ros

scenario test_ros_topic_to_blackboard:
    do parallel:
        test: serial:
            topic_to_blackboard() with:
                keep(it.topic_name == 'bla')
                keep(it.topic_type == 'std_msgs.msg.Bool')
                keep(it.blackboard_variables == '{ \"/test_ros_topic_to_blackboard/end\": \"data\" }')
            emit end
        time_out: serial:
            wait_for_30s: wait elapsed(30s)
            time_out_shutdown: emit fail
