import osc.ros

scenario test_ros_event_to_blackboard:
    do parallel:
        test: serial:
            wait elapsed(2s)
            event_to_blackboard() with:
                keep(it.topic_name == '/bla')
                keep(it.variable_name == '/test_ros_event_to_blackboard/end')
            wait elapsed(60s)
        time_out: serial:
            wait_for_30s: wait elapsed(30s)
            time_out_shutdown: emit fail
