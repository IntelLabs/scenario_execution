import osc.helpers
import osc.ros

scenario test_ros_service_call:
    timeout(30s)
    do serial:
        service_call() with:
            keep(it.service_name == '/bla')
            keep(it.service_type == 'std_srvs.srv.SetBool')
            keep(it.data == '{\"data\": True}')
        wait elapsed(2s)
        emit_arrival: emit end
