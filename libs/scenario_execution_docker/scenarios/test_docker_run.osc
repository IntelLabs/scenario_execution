    do serial:
        docker_run(image: 'ubuntu', command: 'sleep 5', detach: true,remove: true, volumes: ['/srv/nfs4/:/data'])
        docker_run(image: 'ubuntu', command: 'echo hello world', volumes: ['/srv/nfs4/:/data'])
        emit end