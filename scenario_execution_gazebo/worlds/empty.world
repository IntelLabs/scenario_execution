<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <plugin name="state_plugin" filename="libgazebo_ros_state.so"><ros><namespace>gazebo</namespace></ros><update_rate>1.0</update_rate></plugin>
  </world>
</sdf>
